<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.football_warriors" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Football Warriors</name>
    <description>
        A hybrid app to check worldcup 2018 football schedules
    </description>
    <author email="zarif@z-j.se" href="http://cordova.io">
        Zarif Jawad
    </author>
    <author email="fahim@z-j.se" href="http://cordova.io">
        Fahim Nadif
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
